<view class='carousel'>
  <swiper indicator-dots autoplay circular='{{circular}}' interval='{{interval}}' indicator-color='#fff' indicator-active-color='#8C7EEC'>
    <block wx:for="{{imgList}}" wx:key='{{index}}'>
      <swiper-item>
        <image src="{{item}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
</view>