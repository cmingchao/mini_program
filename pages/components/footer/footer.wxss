.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  border-top: 2rpx solid #ccc;
  background-color: #fff;
}
.footer .navigator-item{
  flex-grow: 1;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
}
.footer .navigator-item.actived{
  color: #FFC90F;
}
.footer .navigator-item-2{
  border-left:2rpx solid #ccc;
}