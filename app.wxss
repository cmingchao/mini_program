button {
  height: 70rpx;
  padding: 6rpx 16rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  margin: 0;
}

input, textarea, picker {
  height: 70rpx;
  border: 2rpx solid rgba(213, 213, 213, 0.6);
  border-radius: 10rpx;
  padding-left: 6rpx;
}

textarea {
  width: auto;
  min-height: 160rpx;
}

picker {
  position: relative;
}

image {
  width: auto;
  height: auto;
}

.input-placeholder, .textarea-placeholder {
  color: #999;
  font-size: 30rpx;
}

.bg-white {
  background-color: #fff;
}

.font-medium {
  font-size: 32rpx;
}

.gray {
  color: #666;
}

.light-gray {
  color: #999;
}

.text-center {
  text-align: center;
}

#box {
  background-color: #f5f5f5;
  min-height: 100vh;
  font-size: 28rpx;
  padding-bottom: 120rpx;
  box-sizing: border-box;
}

.noData {
  margin-top: 20rpx;
  font-size: 30rpx;
}
